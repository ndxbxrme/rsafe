{
  "name": "rsafe",
  "version": "4.0.15",
  "description": "ndx-framework web app",
  "main": "app/main.js",
  "scripts": {
    "start": "electron .",
    "pack": "build --win -p always",
    "dev": "grunt build && npm start"
  },
  "build": {
    "appId": "com.ndxbxrme.rsafe",
    "productName": "rsafe"
  },
  "iohook": {
    "targets": [
      "node-57",
      "electron-57"
    ],
    "platforms": [
      "win32"
    ],
    "arches": [
      "x64"
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ndxbxrme/rsafe"
  },
  "author": "ndxbxrme",
  "license": "MIT",
  "homepage": "https://github.com/ndxbxrme/rsafe",
  "devDependencies": {
    "electron": "^1.8.4",
    "electron-builder": "^20.10.0",
    "grunt": "^1.0.2",
    "grunt-cli": "^1.2.0",
    "grunt-contrib-clean": "^1.1.0",
    "grunt-contrib-coffee": "^2.0.0",
    "grunt-contrib-copy": "^1.0.0",
    "grunt-contrib-pug": "^1.0.0",
    "grunt-contrib-stylus": "^1.2.0",
    "grunt-contrib-watch": "^1.0.1",
    "load-grunt-tasks": "^3.5.2"
  },
  "dependencies": {
    "async": "^2.6.0",
    "buffer-slice": "0.0.1",
    "dotty": "^0.1.0",
    "electron-updater": "^2.21.8",
    "gists": "^1.0.1",
    "glob": "^7.1.2",
    "iohook": "^0.1.15",
    "jpeg-js": "^0.3.4",
    "keycode": "^2.2.0",
    "pngjs": "^3.3.3",
    "request": "^2.85.0",
    "robotjs": "^0.5.1",
    "yma": "0.0.13"
  }
}
